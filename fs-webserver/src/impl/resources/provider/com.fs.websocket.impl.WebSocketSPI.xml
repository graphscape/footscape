<config>

	<property name="Object.WS_FACTORY.class" value="com.fs.websocket.impl.jetty.JettyWsFactoryImpl" />

	<config name="WebApp.WSA">
		<context.path value="/wsa" />
	</config>

	<config name="Object.WS_FACTORY">
		<!-- This servletHolder will as the template -->
		<config name="servletHolder.WS_MGR">
			<property name="path" value="/{name}" />
			<prefix name="servlet">
				<property name="class" value="com.fs.websocket.impl.jetty.JettyWsServletImpl" />
			</prefix>
			<config name="init-parameters">
				<maxIdleTime value="$ws.maxIdleTimeMs" />
			</config>

		</config>
	</config>
	<config name="FLASH_POLICY_SERVER">
		<property name="port" value="$flash.policyPort" />
		
	</config>



</config>